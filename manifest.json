{
  "manifest_version": 2,
  "name": "4PDA Downloader",
  "description": "Download from 4PDA",
  "version": "1.0.0",
  "icons": {"128": "icon_128.png"},
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["main.js"]
    }
  ]
}